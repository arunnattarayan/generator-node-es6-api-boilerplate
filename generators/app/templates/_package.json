    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "validator": "^10.11.0"
