{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .build/*",
    "build:dev": "babel src -s -D -d .build",
    "start:dev": "npm run clean && npm run build:dev",
    "lint": "./node_modules/.bin/eslint src/**",
    "lint:test": "./node_modules/.bin/eslint test/**",
    "lint:fix": "./node_modules/.bin/eslint src/** --fix",
    "lint:test:fix": "./node_modules/.bin/eslint test/** --fix",
    "start": "npm run start:dev && node ./.build/server.js",
    "test": "npm run test:coverage:clean && cross-env NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/mocha/bin/mocha --require @babel/register test/**",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:coverage:clean": "rm -rf coverage/*"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "ava": "^0.16.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.3",
    "chai": "^4.2.0",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "ghooks": "^1.2.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-nodemon": "^2.4.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.11",
    "nyc": "^10.3.2",
    "sinon": "^7.3.2",
    "standard": "^8.4.0",
    "supertest": "^4.0.2"
  },
